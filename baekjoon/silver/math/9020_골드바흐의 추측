# 골드바흐의 추측
import sys, math

# 소수인지 아닌지 판별하는 함수
def is_num(i):
    a = 1
    if i > 2 and i % 2 == 0: return False
    for k in range(2, int(math.sqrt(i))+1):
        if i % k == 0:return False
    return True

t = int(sys.stdin.readline().rstrip())
for i in range(t):
    n = int(sys.stdin.readline().rstrip())
    n1, n2 = n//2, n//2 # 입력받은 짝수 값을 반으로 나누기
    
    while n1 > 0:
        if is_num(n1) and is_num(n2): # 나눈값이 둘다 소수라면 서로 가장 근접한 소수
            print(n1, n2)
            break
        else:
            n1-=1                   # 그게 아니라면 서로 +1 -1을 해서 같은 크기만큼 거리를 벌리면서 소수 체크
            n2+=1                     이렇게 하면 서로 가장 근접한 소수를 만날 수 있고 두 소수가 더해졌을 때 n값이 나옴
        
